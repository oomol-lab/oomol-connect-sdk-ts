{
  "name": "oomol-connect-sdk",
  "version": "0.3.0",
  "description": "TypeScript SDK for OOMOL Connect API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oomol/oomol-connect-sdk.git"
  },
  "keywords": [
    "oomol",
    "connect",
    "sdk",
    "typescript",
    "api-client",
    "task-management"
  ],
  "author": "OOMOL",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "typescript": "^5.3.0"
  }
}
